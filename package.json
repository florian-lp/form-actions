{
	"name": "form-actions",
	"version": "1.0.0-beta.6",
	"type": "module",
	"description": "Type-safe form submissions using React server actions.",
	"main": "dist/index.js",
	"module": "dist/index.js",
	"types": "dist/types/index.d.ts",
	"exports": {
		".": "./dist/index.js",
		"./hooks": "./dist/hooks.js"
	},
	"typesVersions": {
		"*": {
			"hooks": [
				"./dist/types/hooks.d.ts"
			]
		}
	},
	"repository": "florian-lp/form-actions",
	"author": "InfinityFX <contact@infinity-fx.com> (https://infinityfx.dev)",
	"license": "GPL-3.0",
	"bugs": "https://github.com/florian-lp/form-actions/issues",
	"homepage": "https://github.com/florian-lp/form-actions#readme",
	"funding": "https://ko-fi.com/infinityfx",
	"keywords": [
		"react",
		"actions",
		"form",
		"state",
		"type-safe",
		"server actions"
	],
	"scripts": {
		"build": "rollup -c --environment NODE_ENV:production",
		"watch": "rollup -c -w"
	},
	"dependencies": {
		"tslib": "^2.6.2"
	},
	"peerDependencies": {
		"react": ">=19.0.0",
		"react-dom": ">=19.0.0"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^25.0.7",
		"@rollup/plugin-node-resolve": "^15.2.3",
		"@rollup/plugin-terser": "^0.4.4",
		"@rollup/plugin-typescript": "^11.1.6",
		"@types/react": "npm:types-react@^19.0.0-rc.0",
		"@types/react-dom": "npm:types-react-dom@^19.0.0-rc.0",
		"react": "^19.0.0-rc-6f23540c7d-20240528",
		"react-dom": "^19.0.0-rc-6f23540c7d-20240528",
		"rollup": "^4.17.1",
		"rollup-plugin-delete": "^2.0.0",
		"tslib": "^2.6.2",
		"typescript": "^5.4.5"
	},
	"sideEffects": false
}
